<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

<header>
    <% include ../partials/header %>
</header>

<main>
    <div id="books" class="books">
        <% jsonData.forEach(data => { %>
            <% console.log(data.id._attributes.nativeid); %>
            <div class="book">
                <h2 class="title"><%- data.id._attributes.nativeid %></h2>
